@model IEnumerable<string>

@{
    Layout = null; 
}

<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" content="width=device-width" />
    <title>Language Features</title>
</head>

<body>
    <ul>
        @foreach (string s in Model)
            if (s.StartsWith("Total"))
            {
                <li><strong>@s</strong></li>
            }
            else
            {
                <li>@s</li>
            }
    </ul>
</body>

</html>